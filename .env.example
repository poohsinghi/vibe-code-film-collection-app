# Environment variables for Docker Compose
# Copy this to .env and fill in your API keys

# API Keys (get these from the respective services)
OMDB_API_KEY=your-omdb-api-key-here
TMDB_API_KEY=your-tmdb-api-key-here

# Database Configuration (Docker will use these)
POSTGRES_DB=film_collection_db
POSTGRES_USER=filmuser
POSTGRES_PASSWORD=filmpass123

# JWT Secret (change this in production)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
